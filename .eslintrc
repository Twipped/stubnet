{
  "ecmaFeatures": {
    "jsx": false
  },
  "env": {
    "browser": false,
    "node": true,
    "amd": true
  },
  "rules": {
    "accessor-pairs": [1, {
      "getWithoutSet": false,
      "setWithoutGet": true
    }],
    "array-bracket-spacing": [2, "never"],
    "arrow-parens": [1, "always"],
    "arrow-spacing": [1, {
      "before": true,
      "after": true
    }],
    "block-scoped-var": 1,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [2, {"properties": "never"}],
    "callback-return": 1,
    "comma-style": [2,"last"],
    "comma-dangle": 0,
    "consistent-return": 0,
    "consistent-this": [1, "self"],
    "constructor-super": 1,
    "complexity": [0, 5],
    "curly": [2, "multi-line"],
    "default-case": 1,
    "dot-notation": [1, {"allowKeywords": true}],
    "eqeqeq": [2, "allow-null"],
    "eol-last": 1,
    "func-names": 0,
    "func-style": 0,
    "generator-star-spacing": [2, {
      "before": true,
      "after": false
    }],
    "guard-for-in": 1,
    "handle-callback-err": 1,
    "id-length": 0,
    "indent": [2, "tab", {
      "SwitchCase": 0,
      "VariableDeclarator": 1
    }],
    "init-declarations": 0,
    "key-spacing": [0, {
      "beforeColon": false,
      "afterColon": true,
      "align": "value"
    }],
    "linebreak-style": 0,
    "max-depth": [1, 3],
    "max-len": [1, 120, 4],
    "max-nested-callbacks": [1, 3],
    "max-params": [1, 5],
    "max-statements": [1, 100],
    "new-cap": [1, {
      "newIsCap": false,
      "capIsNew": true
    }],
    "newline-after-var": 0,
    "no-array-constructor": 2,
    "no-bitwise": 0,
    "no-caller": 2,
    "no-catch-shadow": 1,
    "no-cond-assign": [1, "except-parens"],
    "no-console": 1,
    "no-constant-condition": 1,
    "no-continue": 0,
    "no-control-regex": 0,
    "no-debugger": 1,
    "no-div-regex": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-else-return": 1,
    "no-extra-semi": 1,
    "no-delete-var": 1,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-empty-label": 1,
    "no-eq-null": 0,
    "no-ex-assign": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-extra-parens": [1, "functions"],
    "no-eval": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-func-assign": 1,
    "no-inline-comments": 0,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 1,
    "no-implied-eval": 1,
    "no-irregular-whitespace": 1,
    "no-iterator": 1,
    "no-labels": 1,
    "no-label-var": 1,
    "no-lone-blocks": 1,
    "no-lonely-if": 1,
    "no-loop-func": 1,
    "no-mixed-requires": [1, false],
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multi-spaces": 0,
    "no-multi-str": 1,
    "no-multiple-empty-lines": [1, {"max": 3}],
    "no-native-reassign": 1,
    "no-negated-in-lhs": 1,
    "no-nested-ternary": 1,
    "no-new": 1,
    "no-new-func": 1,
    "no-new-object": 1,
    "no-new-require": 1,
    "no-new-wrappers": 1,
    "no-obj-calls": 1,
    "no-octal": 1,
    "no-octal-escape": 1,
    "no-param-reassign": 0,
    "no-path-concat": 1,
    "no-plusplus": 0,
    "no-process-env": 0,
    "no-process-exit": 1,
    "no-proto": 1,
    "no-redeclare": 2,
    "no-regex-spaces": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "no-shadow": 1,
    "no-shadow-restricted-names": 1,
    "no-spaced-func": 0,
    "no-sparse-arrays": 1,
    "no-sync": 0,
    "no-ternary": 0,
    "no-this-before-super": 0,
    "no-throw-literal": 1,
    "no-trailing-spaces": 1,
    "no-undef-init": 1,
    "no-undefined": 0,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 1,
    "no-unneeded-ternary": 1,
    "no-unreachable": 1,
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "no-useless-call": 1,
    "no-use-before-define": [1, "nofunc"],
    "no-var": 0,
    "no-void": 1,
    "no-with": 1,
    "object-curly-spacing": [1, "always"],
    "object-shorthand": 0,
    "one-var": [2, "never"],
    "operator-assignment": [1, "always"],
    "operator-linebreak": [1, "after"],
    "padded-blocks": 0,
    "prefer-const": 1,
    "prefer-reflect": 0,
    "prefer-spread": 0,
    "quote-props": [1, "consistent"],
    "quotes": [2, "single", "avoid-escape"],
    "radix": 1,
    "require-yield": 2,
    "semi": [2, "always"],
    "semi-spacing": [0, {
      "before": false,
      "after": true
    }],
    "sort-vars": 0,
    "spaced-comment": [1, "always"],
    "space-after-keywords": [2, "always"],
    "space-before-blocks": [1, "always"],
    "space-before-function-paren": [1, {
      "anonymous": "always",
      "named": "always"
    }],
    "space-in-parens": 0,
    "space-infix-ops": [2, {"int32Hint": false}],
    "space-return-throw-case": 1,
    "space-unary-ops": [2, {
      "words":true,
      "nonwords": false
    }],
    "strict": [1, "global"],
    "use-isnan": 1,
    "valid-typeof": 1,
    "vars-on-top": 0,
    "wrap-iife": [2, "inside"],
    "wrap-regex": 0,
    "yoda": [2, "never"]
  }
}
